							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Ардонский район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")
						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Кировский район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")

						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Владикавказ
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")
						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Правобережный район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")
						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Алагирский район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")

				//========Природные ресурсы РСО-А =======
				.economy-republic__block
					.h3 Природные ресурсы РСО-А
					.download-cards-wrapper

						+download-card('Нераспределенный фонд РСО-А_1.')
						+download-card('Минеральные воды РСО-А (бальнеологические группы)_1.')
						+download-card('Месторождения минеральных вод РСО-А_1.')
						+download-card('Лечебно-оздоровительные местности РСО-А (1).')
						

				//========Доступная инфраструктура =======
				.economy-republic__block
					.h3 Доступная инфраструктура для размещения производственных и иных объектов инвесторов
					.download-cards-wrapper
						+download-card('Отчет о деятельности Бизнес инкубатора ИТ-парк Алания за 2015 год', 'pdf')
						+download-card('Отчет ГБУ Бизнес-инкубатор ИТ Парк Алания за 2014 год', 'pdf')
						+download-card('Справка Министерства туризма, предпринимательства и инвестиционной политики РСО-Алания по Бизнес инкубатору', 'pdf')