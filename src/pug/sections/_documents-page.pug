						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Пригородный район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")
						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Ирафский район
							span .pdf
							svg.svg-downl
								use(href="./img/spriteSvg.svg#svg-downl")

						//=====card======
						a(href="#!").download-card
							img(src="./img/icons/pdf.webp", width="62" height="62" alt="img")
							p Дигорский район
							span .pdf
							svg.svg-downl
