.documents-page-content
	.sticky-menu
		.container
			ul#sticky-menu
				li
					a(href="#state-support").active Господдержка
				li
					a(href="#antitrust") Антимонопольный комплаенс
				li
					a(href="#normative") Нормативные документы
				li
					a(href="#economy-republic") Экономика республики

	section.documents
		article.state-support.sticky-menu-section#state-support
			.container
				.h2 Господдержка
				.download-cards-wrapper
					+download-card('О государственной программе Республики Северная Осетия-Алания «Поддержка и развитие малого, среднего предпринимательства и инвестиционной деятельности в Республике Северная Осетия-Алания» на 2020-2024 годы', 'pdf')
					+download-card('Постановление Правительства РФ от 15 апреля 2014 г. N 309 "Об утверждении государственной программы Российской Федерации "Развитие Северо-Кавказского федерального округа"', 'pdf')
		
		
		//===== Антимонопольный комплаенс ======
		article.antitrust.sticky-menu-section#antitrust
			.container
				.h2 Антимонопольный комплаенс
				.download-cards-wrapper
					+download-card('Приказ АО КИР РСО-А N49 от 27.12.2018')
					+download-card('Положение об организации системы внутреннего обеспечения соответствия требованиям антимонопольного законодательства в акционерном обществе «Корпорация инвестиционного развития Республики Северная Осетия-Алания»')
					+download-card('Письмо Минэкономразвития РСО-А 38.1740 от 05.12.2018')
					+download-card('Указ Президента РФ от 21.12.2017г. №618 Об основных направлениях государственной политики по развитию конкуренции')
					+download-card('Распоряжение Правительства РСО-Алания от 04.09.2018г. №295-р О мерах по организации системы внутреннего обеспеченияя соответствия требованиям антимонопольного законодательства деятельности органов исполнительной власти РСО-А')


		//========Нормативные документы========		
		article.normative.sticky-menu-section#normative
			.container
				.h2 Нормативные документы
				.download-cards-wrapper
					+download-card('Годовой отчет за 2022г.', 'pdf')
					+download-card('Годовой отчет АО КИР за 2021.', 'pdf')
					+download-card('Аудиторское заключение за 2020 г.')
					+download-card('Годовой отчет КИР за 2020 г.')
					+download-card('Аудиторское заключение независимого аудитора за 2019г.')
					+download-card('Указ Главы Республики Северная Осетия-Алания от 22 февраля 2017 г. N 39 О порядке принятия решения о предоставлении юридическим лицам земельных участков в аренду без проведения торгов', 'pdf')
					+download-card('Закон Республики Северная Осетия-Алания от 15 апреля 2000 г. N 8-РЗ Об инвестиционной деятельности в Республике Северная Осетия-Алания')
					+download-card('Приложение к распоряжению № 115-р итоги СЭР РСО-А 2019')
		
		//========Экономика республики========
		article.economy-republic.sticky-menu-section#economy-republic
			.container
				.h2 Экономика республики
				
				.economy-republic__block
